{
  "name": "avs-replace",
  "displayName": "AVS Replace",
  "description": "Select a file, enter find/replace pairs, and generate AVS-<basename> output.",
  "version": "0.0.1",
  "publisher": "KirShev",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:avsReplace.start"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "avsReplace.start",
        "title": "AVS: Open Replace Panel"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "echo 'no lint'",
    "test": "echo 'no tests'",
    "package": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "typescript": "^5.6.3",
    "@vscode/vsce": "^3.0.0"
  }
}
